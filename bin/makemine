#!/Users/diego/.virtualenvs/py2/bin/python2
import cPickle
import sys
def dump(info,filename):
    '''Dump python object ['info'] into file ['filename'] using cPickle'''
    output=open(filename,'ab+')
    cPickle.dump(info,output)
    output.close()

def mine():
    output=open('mine','ab+')
    output.close()

dump(' '.join(sys.argv[1:]),'name')
mine()
    
    
