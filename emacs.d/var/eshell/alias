alias apps cd /Applications
alias au a $*
alias config find-file ~/dotfiles/config/emacs.d/config.org
alias desk cd ~/Desktop
alias docs cd ~/Documents
alias down cd ~/Downloads
alias drive cd ~/Google\ Drive
alias drop cd ~/Dropbox\ \(MIT\)
alias edit find-file $1
alias ev view-file $1
alias f find . -iname $1
alias fd find . -type d -iname
alias ff find . -type f -iname
alias filecount ls -1 | wc -l
alias lst ls -t
alias ls ls -X
alias resetwifi networksetup -setairportpower en0 off; sleep 2; networksetup -setairportpower en0 on
alias sz du -hs